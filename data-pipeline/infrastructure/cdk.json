{
  "app": "node bin/data-pipeline.js",
  "watch": {
    "include": ["**"],
    "exclude": ["README.md", "cdk*.json", "jest.config.js", "package*.json", "yarn.lock", "node_modules", "test"]
  },
  "context": {
    "@aws-cdk/aws-lambda:recognizeLayerVersion": true,
    "@aws-cdk/core:checkSecretUsage": true,
    "@aws-cdk/core:target-partitions": ["aws", "aws-cn"]
  }
}